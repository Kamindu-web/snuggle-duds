    {
      "type": "range",
      "id": "hero_subtitle_size",
      "label": "Subtitle size",
      "min": 14,
      "max": 32,
      "step": 2,
      "unit": "px",
      "default": 18
    },
    {
      "type": "color",
      "id": "hero_text_color",
      "label": "Text color",
