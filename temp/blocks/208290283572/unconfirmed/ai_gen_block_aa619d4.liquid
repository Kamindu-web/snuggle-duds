{% doc %}
  @prompt
    I need a T-shirt, hoodie, cap, hat, bag, and shorts mockup design.
    
    Then
    When choosing T-shirts - Adults, Kids
    01) Adults - 2 options Male or Female
    02) Kids - 2 options Female or Male
    
    Or
    When choosing Hoodies - Adults, Kids
    01) Adults - 2 options Male or Female
    02) Kids - 2 options Female or Male
    
    T-shirts and Hoodies
    
    I want to change the t-shirt sample I have uploaded.
    
    I want to choose the unlimited upload option for our designs.
    
    Add an option to change the size and rotation of the gizmo display. The customer needs an option to upload the design. Select the size option, send me an email and get the parts, customer details.
    
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-mockup-customizer-{{ ai_gen_id }} {
    display: block;
    width: {{ block.settings.desktop_width_percent }}%;
    max-width: 100%;
    padding: 40px;
    background-color: {{ block.settings.background_color }};
    border-radius: 8px;
  }

  .ai-mockup-customizer__container-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 40px;
  }

  .ai-mockup-customizer__preview-section-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .ai-mockup-customizer__preview-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    aspect-ratio: 1;
    background-color: #f5f5f5;
    border-radius: 8px;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-mockup-customizer__mockup-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: contain;
  }

  .ai-mockup-customizer__design-canvas-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    pointer-events: none;
  }

  .ai-mockup-customizer__design-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: contain;
  }

  .ai-mockup-customizer__placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #f5f5f5;
  }

  .ai-mockup-customizer__placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-mockup-customizer__controls-section-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 24px;
  }

  .ai-mockup-customizer__heading-{{ ai_gen_id }} {
    margin: 0 0 8px;
    font-size: 24px;
    color: {{ block.settings.text_color }};
  }

  .ai-mockup-customizer__subheading-{{ ai_gen_id }} {
    margin: 0 0 16px;
    font-size: 14px;
    color: {{ block.settings.text_color }};
    opacity: 0.7;
  }

  .ai-mockup-customizer__field-group-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 8px;
  }

  .ai-mockup-customizer__label-{{ ai_gen_id }} {
    font-size: 14px;
    font-weight: 600;
    color: {{ block.settings.text_color }};
  }

  .ai-mockup-customizer__select-{{ ai_gen_id }},
  .ai-mockup-customizer__input-{{ ai_gen_id }} {
    padding: 12px 16px;
    border: 1px solid #ddd;
    border-radius: 4px;
    font-size: 14px;
    background-color: #fff;
  }

  .ai-mockup-customizer__upload-area-{{ ai_gen_id }} {
    border: 2px dashed #ddd;
    border-radius: 8px;
    padding: 24px;
    text-align: center;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-mockup-customizer__upload-area-{{ ai_gen_id }}:hover {
    border-color: {{ block.settings.button_color }};
    background-color: rgba(0, 0, 0, 0.02);
  }

  .ai-mockup-customizer__upload-input-{{ ai_gen_id }} {
    display: none;
  }

  .ai-mockup-customizer__upload-text-{{ ai_gen_id }} {
    font-size: 14px;
    color: {{ block.settings.text_color }};
    margin-bottom: 8px;
  }

  .ai-mockup-customizer__upload-hint-{{ ai_gen_id }} {
    font-size: 12px;
    color: {{ block.settings.text_color }};
    opacity: 0.6;
  }

  .ai-mockup-customizer__uploaded-designs-{{ ai_gen_id }} {
    display: flex;
    flex-wrap: wrap;
    gap: 12px;
    margin-top: 12px;
  }

  .ai-mockup-customizer__design-thumb-{{ ai_gen_id }} {
    position: relative;
    width: 80px;
    height: 80px;
    border-radius: 4px;
    overflow: hidden;
    border: 2px solid #ddd;
  }

  .ai-mockup-customizer__design-thumb-{{ ai_gen_id }}.active {
    border-color: {{ block.settings.button_color }};
  }

  .ai-mockup-customizer__design-thumb-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
    cursor: pointer;
  }

  .ai-mockup-customizer__design-remove-{{ ai_gen_id }} {
    position: absolute;
    top: 4px;
    right: 4px;
    width: 20px;
    height: 20px;
    background-color: rgba(0, 0, 0, 0.7);
    color: #fff;
    border: none;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 12px;
  }

  .ai-mockup-customizer__slider-group-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 8px;
  }

  .ai-mockup-customizer__slider-label-{{ ai_gen_id }} {
    display: flex;
    justify-content: space-between;
    font-size: 14px;
    color: {{ block.settings.text_color }};
  }

  .ai-mockup-customizer__slider-{{ ai_gen_id }} {
    width: 100%;
    height: 6px;
    border-radius: 3px;
    background: #ddd;
    outline: none;
    -webkit-appearance: none;
  }

  .ai-mockup-customizer__slider-{{ ai_gen_id }}::-webkit-slider-thumb {
    -webkit-appearance: none;
    appearance: none;
    width: 18px;
    height: 18px;
    border-radius: 50%;
    background: {{ block.settings.button_color }};
    cursor: pointer;
  }

  .ai-mockup-customizer__slider-{{ ai_gen_id }}::-moz-range-thumb {
    width: 18px;
    height: 18px;
    border-radius: 50%;
    background: {{ block.settings.button_color }};
    cursor: pointer;
    border: none;
  }

  .ai-mockup-customizer__button-{{ ai_gen_id }} {
    padding: 14px 28px;
    background-color: {{ block.settings.button_color }};
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: 4px;
    font-size: 16px;
    font-weight: 600;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-mockup-customizer__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  .ai-mockup-customizer__success-message-{{ ai_gen_id }} {
    padding: 16px;
    background-color: #d4edda;
    color: #155724;
    border-radius: 4px;
    font-size: 14px;
    display: none;
  }

  .ai-mockup-customizer__success-message-{{ ai_gen_id }}.show {
    display: block;
  }

  .ai-mockup-customizer__error-message-{{ ai_gen_id }} {
    padding: 16px;
    background-color: #f8d7da;
    color: #721c24;
    border-radius: 4px;
    font-size: 14px;
    display: none;
  }

  .ai-mockup-customizer__error-message-{{ ai_gen_id }}.show {
    display: block;
  }

  @media screen and (max-width: 768px) {
    .ai-mockup-customizer__container-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 24px;
    }

    .ai-mockup-customizer-{{ ai_gen_id }} {
      padding: 20px;
    }
  }
{% endstyle %}

<mockup-customizer-{{ ai_gen_id }} class="ai-mockup-customizer-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-mockup-customizer__container-{{ ai_gen_id }}">
    <div class="ai-mockup-customizer__preview-section-{{ ai_gen_id }}">
      <h2 class="ai-mockup-customizer__heading-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
      <p class="ai-mockup-customizer__subheading-{{ ai_gen_id }}">{{ block.settings.subheading }}</p>
      
      <div class="ai-mockup-customizer__preview-wrapper-{{ ai_gen_id }}">
        <img
          class="ai-mockup-customizer__mockup-image-{{ ai_gen_id }}"
          src=""
          alt="Product mockup"
          style="display: none;"
        >
        <div class="ai-mockup-customizer__placeholder-{{ ai_gen_id }}">
          {{ 'product-1' | placeholder_svg_tag }}
        </div>
        <div class="ai-mockup-customizer__design-canvas-{{ ai_gen_id }}" style="width: 200px; height: 200px;">
          <img
            class="ai-mockup-customizer__design-image-{{ ai_gen_id }}"
            src=""
            alt="Your design"
            style="display: none;"
          >
        </div>
      </div>
    </div>

    <div class="ai-mockup-customizer__controls-section-{{ ai_gen_id }}">
      {%- form 'contact' -%}
        <input type="hidden" name="contact[subject]" value="Custom Product Design Request">
        
        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Product category</label>
          <select class="ai-mockup-customizer__select-{{ ai_gen_id }}" name="contact[product_category]" data-category-select>
            <option value="">Select a product</option>
            <option value="tshirt">T-shirt</option>
            <option value="hoodie">Hoodie</option>
            <option value="cap">Cap</option>
            <option value="hat">Hat</option>
            <option value="bag">Bag</option>
            <option value="shorts">Shorts</option>
          </select>
        </div>

        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}" data-age-group style="display: none;">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Age group</label>
          <select class="ai-mockup-customizer__select-{{ ai_gen_id }}" name="contact[age_group]" data-age-select>
            <option value="">Select age group</option>
            <option value="adults">Adults</option>
            <option value="kids">Kids</option>
          </select>
        </div>

        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}" data-gender-group style="display: none;">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Gender</label>
          <select class="ai-mockup-customizer__select-{{ ai_gen_id }}" name="contact[gender]" data-gender-select>
            <option value="">Select gender</option>
            <option value="male">Male</option>
            <option value="female">Female</option>
          </select>
        </div>

        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Upload your designs</label>
          <div class="ai-mockup-customizer__upload-area-{{ ai_gen_id }}" data-upload-area>
            <input
              type="file"
              class="ai-mockup-customizer__upload-input-{{ ai_gen_id }}"
              accept="image/*"
              multiple
              data-file-input
            >
            <div class="ai-mockup-customizer__upload-text-{{ ai_gen_id }}">Click to upload designs</div>
            <div class="ai-mockup-customizer__upload-hint-{{ ai_gen_id }}">PNG, JPG, SVG (unlimited files)</div>
          </div>
          <div class="ai-mockup-customizer__uploaded-designs-{{ ai_gen_id }}" data-designs-container></div>
        </div>

        <div class="ai-mockup-customizer__slider-group-{{ ai_gen_id }}">
          <div class="ai-mockup-customizer__slider-label-{{ ai_gen_id }}">
            <span>Design size</span>
            <span data-size-value>100%</span>
          </div>
          <input
            type="range"
            class="ai-mockup-customizer__slider-{{ ai_gen_id }}"
            min="50"
            max="200"
            value="100"
            data-size-slider
            name="contact[design_size]"
          >
        </div>

        <div class="ai-mockup-customizer__slider-group-{{ ai_gen_id }}">
          <div class="ai-mockup-customizer__slider-label-{{ ai_gen_id }}">
            <span>Design rotation</span>
            <span data-rotation-value>0°</span>
          </div>
          <input
            type="range"
            class="ai-mockup-customizer__slider-{{ ai_gen_id }}"
            min="0"
            max="360"
            value="0"
            data-rotation-slider
            name="contact[design_rotation]"
          >
        </div>

        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Your name</label>
          <input
            type="text"
            class="ai-mockup-customizer__input-{{ ai_gen_id }}"
            name="contact[name]"
            required
          >
        </div>

        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Email address</label>
          <input
            type="email"
            class="ai-mockup-customizer__input-{{ ai_gen_id }}"
            name="contact[email]"
            required
          >
        </div>

        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Phone number</label>
          <input
            type="tel"
            class="ai-mockup-customizer__input-{{ ai_gen_id }}"
            name="contact[phone]"
          >
        </div>

        <div class="ai-mockup-customizer__field-group-{{ ai_gen_id }}">
          <label class="ai-mockup-customizer__label-{{ ai_gen_id }}">Additional notes</label>
          <textarea
            class="ai-mockup-customizer__input-{{ ai_gen_id }}"
            name="contact[message]"
            rows="4"
            placeholder="Size preferences, quantity, delivery date, etc."
          ></textarea>
        </div>

        <input type="hidden" name="contact[design_files]" data-files-data>

        <button type="submit" class="ai-mockup-customizer__button-{{ ai_gen_id }}">
          {{ block.settings.button_text }}
        </button>

        {%- if form.posted_successfully? -%}
          <div class="ai-mockup-customizer__success-message-{{ ai_gen_id }} show">
            {{ block.settings.success_message }}
          </div>
        {%- endif -%}

        {%- if form.errors -%}
          <div class="ai-mockup-customizer__error-message-{{ ai_gen_id }} show">
            Please fill in all required fields correctly.
          </div>
        {%- endif -%}
      {%- endform -%}
    </div>
  </div>
</mockup-customizer-{{ ai_gen_id }}>

<script>
  (function() {
    class MockupCustomizer{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.mockupImages = {
          tshirt: {
            adults: {
              male: '{{ block.settings.tshirt_adult_male | image_url: width: 800 }}',
              female: '{{ block.settings.tshirt_adult_female | image_url: width: 800 }}'
            },
            kids: {
              male: '{{ block.settings.tshirt_kids_male | image_url: width: 800 }}',
              female: '{{ block.settings.tshirt_kids_female | image_url: width: 800 }}'
            }
          },
          hoodie: {
            adults: {
              male: '{{ block.settings.hoodie_adult_male | image_url: width: 800 }}',
              female: '{{ block.settings.hoodie_adult_female | image_url: width: 800 }}'
            },
            kids: {
              male: '{{ block.settings.hoodie_kids_male | image_url: width: 800 }}',
              female: '{{ block.settings.hoodie_kids_female | image_url: width: 800 }}'
            }
          },
          cap: '{{ block.settings.cap_mockup | image_url: width: 800 }}',
          hat: '{{ block.settings.hat_mockup | image_url: width: 800 }}',
          bag: '{{ block.settings.bag_mockup | image_url: width: 800 }}',
          shorts: '{{ block.settings.shorts_mockup | image_url: width: 800 }}'
        };
        
        this.uploadedDesigns = [];
        this.currentDesignIndex = 0;
      }

      connectedCallback() {
        this.categorySelect = this.querySelector('[data-category-select]');
        this.ageGroup = this.querySelector('[data-age-group]');
        this.ageSelect = this.querySelector('[data-age-select]');
        this.genderGroup = this.querySelector('[data-gender-group]');
        this.genderSelect = this.querySelector('[data-gender-select]');
        this.mockupImage = this.querySelector('.ai-mockup-customizer__mockup-image-{{ ai_gen_id }}');
        this.placeholder = this.querySelector('.ai-mockup-customizer__placeholder-{{ ai_gen_id }}');
        this.uploadArea = this.querySelector('[data-upload-area]');
        this.fileInput = this.querySelector('[data-file-input]');
        this.designsContainer = this.querySelector('[data-designs-container]');
        this.designImage = this.querySelector('.ai-mockup-customizer__design-image-{{ ai_gen_id }}');
        this.designCanvas = this.querySelector('.ai-mockup-customizer__design-canvas-{{ ai_gen_id }}');
        this.sizeSlider = this.querySelector('[data-size-slider]');
        this.sizeValue = this.querySelector('[data-size-value]');
        this.rotationSlider = this.querySelector('[data-rotation-slider]');
        this.rotationValue = this.querySelector('[data-rotation-value]');
        this.filesData = this.querySelector('[data-files-data]');

        this.setupEventListeners();
      }

      setupEventListeners() {
        this.categorySelect.addEventListener('change', () => this.handleCategoryChange());
        this.ageSelect.addEventListener('change', () => this.updateMockup());
        this.genderSelect.addEventListener('change', () => this.updateMockup());
        
        this.uploadArea.addEventListener('click', () => this.fileInput.click());
        this.fileInput.addEventListener('change', (e) => this.handleFileUpload(e));
        
        this.sizeSlider.addEventListener('input', (e) => {
          this.sizeValue.textContent = e.target.value + '%';
          this.updateDesignTransform();
        });
        
        this.rotationSlider.addEventListener('input', (e) => {
          this.rotationValue.textContent = e.target.value + '°';
          this.updateDesignTransform();
        });
      }

      handleCategoryChange() {
        const category = this.categorySelect.value;
        
        if (category === 'tshirt' || category === 'hoodie') {
          this.ageGroup.style.display = 'flex';
          this.ageSelect.value = '';
          this.genderGroup.style.display = 'none';
          this.genderSelect.value = '';
        } else {
          this.ageGroup.style.display = 'none';
          this.genderGroup.style.display = 'none';
          this.ageSelect.value = '';
          this.genderSelect.value = '';
        }
        
        this.updateMockup();
      }

      updateMockup() {
        const category = this.categorySelect.value;
        const age = this.ageSelect.value;
        const gender = this.genderSelect.value;
        
        if (!category) {
          this.mockupImage.style.display = 'none';
          this.placeholder.style.display = 'flex';
          return;
        }
        
        let imageUrl = '';
        
        if (category === 'tshirt' || category === 'hoodie') {
          if (age && gender) {
            imageUrl = this.mockupImages[category][age][gender];
            this.genderGroup.style.display = 'flex';
          } else if (age) {
            this.genderGroup.style.display = 'flex';
            return;
          } else {
            return;
          }
        } else {
          imageUrl = this.mockupImages[category];
        }
        
        if (imageUrl && imageUrl !== '') {
          this.mockupImage.src = imageUrl;
          this.mockupImage.style.display = 'block';
          this.placeholder.style.display = 'none';
        } else {
          this.mockupImage.style.display = 'none';
          this.placeholder.style.display = 'flex';
        }
      }

      handleFileUpload(e) {
        const files = Array.from(e.target.files);
        
        files.forEach(file => {
          const reader = new FileReader();
          reader.onload = (event) => {
            this.uploadedDesigns.push({
              url: event.target.result,
              name: file.name
            });
            this.renderDesigns();
            if (this.uploadedDesigns.length === 1) {
              this.selectDesign(0);
            }
          };
          reader.readAsDataURL(file);
        });
        
        e.target.value = '';
      }

      renderDesigns() {
        this.designsContainer.innerHTML = '';
        
        this.uploadedDesigns.forEach((design, index) => {
          const thumb = document.createElement('div');
          thumb.className = 'ai-mockup-customizer__design-thumb-{{ ai_gen_id }}';
          if (index === this.currentDesignIndex) {
            thumb.classList.add('active');
          }
          
          thumb.innerHTML = `
            <img
              src="${design.url}"
              alt="${design.name}"
              class="ai-mockup-customizer__design-thumb-image-{{ ai_gen_id }}"
            >
            <button
              type="button"
              class="ai-mockup-customizer__design-remove-{{ ai_gen_id }}"
              data-remove="${index}"
            >×</button>
          `;
          
          thumb.querySelector('img').addEventListener('click', () => this.selectDesign(index));
          thumb.querySelector('[data-remove]').addEventListener('click', (e) => {
            e.stopPropagation();
            this.removeDesign(index);
          });
          
          this.designsContainer.appendChild(thumb);
        });
        
        this.updateFilesData();
      }

      selectDesign(index) {
        this.currentDesignIndex = index;
        const design = this.uploadedDesigns[index];
        
        if (design) {
          this.designImage.src = design.url;
          this.designImage.style.display = 'block';
        }
        
        this.renderDesigns();
      }

      removeDesign(index) {
        this.uploadedDesigns.splice(index, 1);
        
        if (this.currentDesignIndex >= this.uploadedDesigns.length) {
          this.currentDesignIndex = Math.max(0, this.uploadedDesigns.length - 1);
        }
        
        if (this.uploadedDesigns.length === 0) {
          this.designImage.style.display = 'none';
        } else {
          this.selectDesign(this.currentDesignIndex);
        }
        
        this.renderDesigns();
      }

      updateDesignTransform() {
        const size = this.sizeSlider.value;
        const rotation = this.rotationSlider.value;
        
        this.designCanvas.style.width = (200 * size / 100) + 'px';
        this.designCanvas.style.height = (200 * size / 100) + 'px';
        this.designCanvas.style.transform = `translate(-50%, -50%) rotate(${rotation}deg)`;
      }

      updateFilesData() {
        const fileNames = this.uploadedDesigns.map(d => d.name).join(', ');
        this.filesData.value = fileNames;
      }
    }

    customElements.define('mockup-customizer-{{ ai_gen_id }}', MockupCustomizer{{ ai_gen_id }});
  })();
</script>

{% schema %}
{
  "name": "Product mockup customizer",
  "settings": [
    {
      "type": "header",
      "content": "Content"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Design your custom product"
    },
    {
      "type": "text",
      "id": "subheading",
      "label": "Subheading",
      "default": "Upload your design and see it on your chosen product"
    },
    {
      "type": "text",
      "id": "button_text",
      "label": "Button text",
      "default": "Submit design request"
    },
    {
      "type": "text",
      "id": "success_message",
      "label": "Success message",
      "default": "Thank you! We'll review your design and contact you shortly."
    },
    {
      "type": "header",
      "content": "T-shirt mockups"
    },
    {
      "type": "image_picker",
      "id": "tshirt_adult_male",
      "label": "Adult male"
